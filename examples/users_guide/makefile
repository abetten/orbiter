			-width 60 -width_10 6 -end \
		-define C -combinatorial_object \
			-label Praeger Praeger \
			-file_of_incidence_geometries \
				Praeger_45_flags.txt 45 45 540 \
		-end \
		-with C -do \
		-combinatorial_object_activity \
			-canonical_form \
				-save_ago \
				-save_transversal \
				-nauty_control -end \
			-end \
		-end \
		-with C -do \
		-combinatorial_object_activity \
			-post_processing \
		-end \
		-assign Object -from \
		-with C -do \
		-combinatorial_object_activity \
			-get_combo_with_group 0 \
		-end \
