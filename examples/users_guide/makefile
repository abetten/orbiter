		-define F -finite_field -q 2 -end \
		-define P -projective_space -n 3 -field F -v 0 -end \
		-with P -do \
		-projective_space_activity \
		-table_of_cubic_surfaces_compute_properties \
			poly_orbits_d3_n3_q2.csv 2 0 \
		-end 


