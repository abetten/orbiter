			$(AUT_SYM_6_GROUP_ORDER) \
			$(AUT_SYM_6_BASE) \
			$(AUT_SYM_6_NB_GENS) \
			gens \
		-end \
		-with G -do \
		-group_theoretic_activity \
			-report \
		-end
	pdflatex AutSym6_report.tex
	$(OPEN) AutSym6_report.pdf


