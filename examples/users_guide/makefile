	$(ORBITER) -v 3 \
		-define F -finite_field -q 5 -end \
		-define f -formula \
			"test_formula" "test\_formula" "" \
			$(TEST_FORMULA) \
		-with F -do -finite_field_activity \
			-evaluate f "a=2,b=3,x=4" -end

