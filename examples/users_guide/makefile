		-define F -finite_field -q 256 -end \
		-define Echo -crc_code -type Echo -block_length 25 -end \
		-define CRC32 -crc_code -type crc32 -block_length 25 -end \
		-with F -do \
			-coding_theoretic_activity \
				-crc_compare \
					Oben.mp4 \
					Echo \
					CRC32 \
					5 \
		-end


