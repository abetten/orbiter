			-homogeneous_of_degree 2 \
			-monomial_ordering_lex \
			-variables "x0,x1,x2" "x_0,x_1,x_2" \
			-end \
		-define C -combinatorial_object \
			-label random_arc random\_arc \
			-set_of_points "3,33,40,83,102" \
		-end \
		-with C -do \
		-combinatorial_object_activity \
			-ideal R \
		-end

