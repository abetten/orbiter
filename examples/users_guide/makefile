Hamming_code:
	$(ORBITER) -v 2 \
		-define F -finite_field -q 2 -end \
		-define v -vector -field F -format 3 \
			-dense $(SIMPLEX_CODE_GENERATOR) \
		-end \
		-define C -code -field F \
			-generator_matrix v \
			-dual -end \
		-end \
		-with C -do -coding_theoretic_activity \
			-export_magma Hamming.magma \
		-end 

