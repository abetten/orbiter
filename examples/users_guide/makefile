linsp6:
	$(ORBITER) -v 4 \
		-define A -vector -format 1 -dense "15,10,6,3,1" -end \
		-define D -diophant -label linsp6 \
		-coefficient_matrix A \
		-RHS "mult=1,EQ=15" \
		-x_min_global 0 \
		-x_max_global 15 \
		-end \
		-with D -do \
			-diophant_activity -solve_mckay \
		-end
	
# Found 15 solutions with 22 backtrack steps




