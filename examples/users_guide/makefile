			-end \
		-define Y0 -symbolic_object \
			-field F \
			-text "3*(1-x^4)+4*(1-(x-2)^4)+1*(1-(x-3)^4)+2*(1-(x-4)^4)" \
			-end \
		-define Y -symbolic_object \
			-field F \
			-text "Y0" \
			-end \
		-with P -do \
