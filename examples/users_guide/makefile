				-H "H31" $(PGL_4_5_SUBGROUP_31_ME) -end \
				-N "N31" $(PGL_4_5_SUBGROUP_31_ME_NORMALIZER) -end \
			-end \
		-define PWF -packing_choose_fixed_points PW 0 -end \
		-define L -packing_long_orbits PWF \
			-orbit_length 31 -create_graphs \
			-end \
		-print_symbols


