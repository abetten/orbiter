			-scale 0.1 \
		-end \
		-define GC -graph_classification \
			-n 5 \
			-poset_classification_control \
				-problem_label graphs_v5 \
				-depth 10 \
				-draw_options Do \
			-end \
		-end \
		-with GC -do \
		-graph_classification_activity \
			-list_graphs_at_level 5 5 \
		-end \
