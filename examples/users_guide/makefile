		-define B -vector -field F -dense $(CRC_POLY_DELTA_DENSE) -end \
		-with F -do \
		-coding_theoretic_activity \
			-write_code_for_division \
			delta A B \
		-end
	g++ crc_delta.cpp -o crc_delta.out
	./crc_delta.out

