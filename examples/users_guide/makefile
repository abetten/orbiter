		-loop L 0 3 1 \
			-draw_matrix \
				-input_csv_file RM_m3_distance_H_8_16_value%L.csv \
				-box_width 25 -bit_depth 8 \
				-partition 5 16 16 \
			-end \
		-end_loop L
