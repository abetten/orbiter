quartic_curves_q13_classify_nauty:
	$(ORBITER) -v 3 \
		-define F -finite_field -q 13 -end \
		-define P -projective_space -n 3 -field F -end \
		-loop L 0 $(NB_CUBIC_SURFACES_Q13) 1 \
			-define S_%L -cubic_surface -space P -catalogue %L -end \
		-end_loop L \
		-print_symbols \
		-loop L 0 $(NB_CUBIC_SURFACES_Q13) 1 \
			-with S_%L -do \
			-cubic_surface_activity \
				-export_all_quartic_curves \
			-end \
		-end_loop L \
		-print_symbols
	$(ORBITER) -v 3 \
		-csv_file_concatenate_from_mask 0 3 \
			surface_catalogue_q13_iso%ld_quartics.csv \
			quartics_q13.csv
	$(ORBITER) -v 3 \
		-define F -finite_field -q 13 -end \
		-define P -projective_space -n 2 -field F -v 0 -end \
		-define R -polynomial_ring \
			-field F \
			-number_of_variables 3 \
			-homogeneous_of_degree 4 \
			-monomial_ordering_partition \
			-variables "y0,y1,y2" "y_0,y_1,y_2" \
		-end \
		-define O -orbits -classification_by_canonical_form \
			-space P \
			-ring R \
			-input_fname_mask quartics_q13.csv \
			-nb_files 1 \
			-output_fname quartic_curves_q13 \
			-label_po "PO" \
			-label_so "orbit" \
			-carry_through "SEQN" \
			-carry_through "SEQNALG" \
			-carry_through "PT" \
			-carry_through "PTCOEFF" \
			-carry_through "curve" \
			-carry_through "CURVEALG" \
			-carry_through "nb_pts_on_curve" \
			-label_equation_by_coefficients "curve" \
			-label_points "pts_on_curve" \
			-label_lines "bitangents" \
			-carry_through "NB_E" \
			-carry_through "NB_K" \
			-carry_through "Kovalevski_pts" \
			-carry_through "NB_DOUBLE" \
			-carry_through "NB_SINGLE" \
			-carry_through "NB_ZERO" \
			-nauty_control \
				-save_orbit_of_equations equations_ \
			-end \
			-end \
		-end \
		-define ro \
			-poset_classification_report_options \
				-fname quartic_curves_q13 \
		-end \
		-with O -do -orbits_activity \
			-report \
			-report_options ro \
		-end
	$(PDFLATEX) quartic_curves_q13_orbits.tex
	$(OPEN) quartic_curves_q13_orbits.pdf




Klein_cubic_threefold_q3:
	$(ORBITER) -v 10 \
		-define F -finite_field -q 3 -end \
		-define P -projective_space -n 4 -field F -v 0 -end \
		-define R -polynomial_ring \
			-field F \
			-number_of_variables 5 \
			-homogeneous_of_degree 3 \
			-monomial_ordering_partition \
			-variables "X0,X1,X2,X3,X4" "X_0,X_1,X_2,X_3,X_4" \
		-end \
		-define V -variety \
			-projective_space P \
			-ring R \
			-equation_in_algebraic_form $(KLEIN_CUBIC_THREEFOLD_EQN) \
			-label_txt Klein3_q3 \
			-label_tex Klein3\_q3 \
		-end \
		-with V -do -variety_activity -compute_group -nauty_control -end -end \
		-with V -do -variety_activity -singular_points -end \
		-with V -do -variety_activity -report -end
	$(PDFLATEX) variety_Klein3_q3_report.tex
	$(OPEN) variety_Klein3_q3_report.pdf

