		-define D -diophant \
		-label "KM_PGGL_2_32_KM_7_8_system" \
		-coefficient_matrix A \
		-RHS_constant "EQ=10" \
		-x_min_global 0 -x_max_global 1 \
		-end \
		-with D -do \
			-diophant_activity -solve_mckay \
		-end





